{
  "name": "feedback-board",
  "homepage": "https://sosukii.github.io/avent-board",
  "proxy": "easy-feedback.up.railway.app",
  "version": "1.0.0",
  "description": "Client board for feedback management on wildberries",
  "productName": "Easy Feedback",
  "author": "Svetlana Trukhina <sweta4ca@mail.ru>",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "build": "quasar build",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "prepare": "husky install && npx husky add .husky/pre-commit \"npx lint-staged --relative\"",
    "deploy": "gh-pages -d dist/spa"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "axios": "^1.2.1",
    "gh-pages": "^6.0.0",
    "lottie-web-vue": "^2.0.7",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-clipboard3": "^2.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.3.0",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "lint-staged": {
    "*.{js,ts,vue,scss,html,css}": [
      "prettier --write --config .prettierrc"
    ],
    "*.{js,ts,vue}": [
      "eslint --fix"
    ]
  }
}
